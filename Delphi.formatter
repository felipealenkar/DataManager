[Indentation]
IndentationSize=2
IndentClass=true
IndentRecord=true
IndentBegin=true

[LineBreaks]
BreakAfterEachParameter=false
BreakAfterEachStatement=false
BreakLinesOnlyIfLengthExceeds=true
MaxLineLength=300

[Capitalization]
UpperCaseKeywords=true

[Spacing]
SpaceAfterComma=true
SpaceAroundOperators=true